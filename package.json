    "glob": "^11.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.9.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^6.0.1"
  }
